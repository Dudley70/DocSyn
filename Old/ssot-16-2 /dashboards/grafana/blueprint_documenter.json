{
  "title": "Blueprint \u2014 documenter",
  "templating": {
    "list": [
      {
        "name": "env",
        "type": "query",
        "query": "label_values(job)",
        "current": {
          "text": "prod",
          "value": "prod"
        }
      },
      {
        "name": "service",
        "type": "query",
        "query": "label_values(documenter_runs_total, service)",
        "current": {
          "text": "documenter",
          "value": "documenter"
        }
      },
      {
        "name": "DS_PROM",
        "type": "datasource",
        "query": "prometheus"
      },
      {
        "name": "DS_LOKI",
        "type": "datasource",
        "query": "loki"
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Success rate",
      "targets": [
        {
          "expr": "sum(rate(documenter_runs_success_total[5m]))/sum(rate(documenter_runs_total[5m]))",
          "datasource": {
            "type": "prometheus",
            "uid": "$DS_PROM"
          }
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      }
    },
    {
      "type": "timeseries",
      "title": "Latency p95",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(documenter_latency_seconds_bucket[5m])) by (le))",
          "datasource": {
            "type": "prometheus",
            "uid": "$DS_PROM"
          }
        }
      ],
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 10,
        "h": 4
      }
    },
    {
      "type": "logs",
      "title": "Errors",
      "targets": [
        {
          "expr": "{service=\"documenter\"} |= \"error\"",
          "datasource": {
            "type": "loki",
            "uid": "$DS_LOKI"
          }
        }
      ],
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 8,
        "h": 8
      }
    }
  ],
  "schemaVersion": 36,
  "version": 1
}